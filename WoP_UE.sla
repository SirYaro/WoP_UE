                <ITEXT CH="3x Czaszka"/>
                <para PARENT="Lista wypunktowana cond (-1)"/>
                <ITEXT CH="1x Zagraj kartę"/>
                <para PARENT="Lista wypunktowana cond (-1)"/>
                <ITEXT CPARENT="pogrubienie 11pt *" CH="Zdolności przywódcze"/>
                <ITEXT CH=" armii to suma liczby dowódców (lub Nazgûli) i wartości zdolności przywódczych postaci (wskazanych na kartach Postaci) biorących udział w bitwie. Zdolności przywódcze wskazują, iloma kośćmi gracz może wykonać ponowny rzut podczas "/>
                <ITEXT CPARENT="pogrubienie 11pt *" CH="przerzutu"/>
