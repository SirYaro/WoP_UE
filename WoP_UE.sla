        <PAGEOBJECT XPOS="681.102992125984" YPOS="5.82803149606299" OwnPage="0" ItemID="1518387786" PTYPE="2" WIDTH="623.622047244095" HEIGHT="870.236220472441" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.229525965124805" LOCALSCY="0.232434887946699" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="0" Pagenumber="0" PFILE="../../../../../PnP/todo/Wojna o Pierścień Ultimate/skład/img/WoP_instrukcja_pusta_p1.png" PRFILE="Embedded Artifex Software sRGB ICC Profile" IRENDER="0" EPROF="Embedded Artifex Software sRGB ICC Profile" path="M0 0 L623.622 0 L623.622 870.236 L0 870.236 L0 0 Z" copath="M0 0 L623.622 0 L623.622 870.236 L0 870.236 L0 0 Z" gXpos="681.102992125984" gYpos="5.82803149606299" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
