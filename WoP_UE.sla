                <para PARENT="Lista wypunktowana cond"/>
                <ITEXT CH="1x Czaszka"/>
                <para PARENT="Lista wypunktowana cond"/>
                <ITEXT CH="1x 2 Czaszki"/>
                <para PARENT="Lista wypunktowana cond"/>
                <ITEXT CH="Podczas późniejszych rund bitwy gracze stosują się do normalnych zasad."/>
