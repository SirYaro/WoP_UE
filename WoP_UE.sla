        <PAGEOBJECT XPOS="995.295118110236" YPOS="1221.91086614173" OwnPage="2" ItemID="1941907994" PTYPE="4" WIDTH="255.256692913386" HEIGHT="500.031496062992" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="7.08661417322835" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L255.257 0 L255.257 500.031 L0 500.031 L0 0 Z" copath="M0 0 L255.257 0 L255.257 500.031 L0 500.031 L0 0 Z" gXpos="995.295118110236" gYpos="3867.5788976378" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="1945455786" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="ROZDZIAŁ II:"/>
                <para PARENT="Rozdział nr"/>
                <ITEXT CH="ELEMENTY GRY"/>
                <para PARENT="Rozdział tytuł"/>
                <ITEXT CH="LISTA ELEMENTÓW DO WARIANTU ULTIMATE REWORK"/>
                <para PARENT="Box tytuł 0"/>
                <ITEXT CH="Poniżej znajduje się lista elementów, których będziesz potrzebował z gry podstawowej:"/>
                <para/>
                <ITEXT CH="1 plansza podzielona na dwie części"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="6 kości akcji Wolnych Ludów"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="10 kości akcji gracza Cienia"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="205 figurek przedstawiających armie i postacie "/>
                <ITEXT CPARENT="kursywa" CH="Wojny o Pierścień"/>
                <ITEXT CH=", w tym:"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="90 figurek jednostek Cienia"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="75 figurek jednostek Wolnych Ludów"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="20 figurek dowódców Wolnych Ludów i 7 figurek Nazgûli"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="1 figurka Nazgûla (którą należy oznaczyć dla odróżnienia, użyta jako Ognisty Smok)"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="8 figurek Powierników Pierścienia (Froda i Sama) oraz ich Towarzyszy"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="4 figurki Sług Cienia (Khamûl, Rzecznik Saurona, Gollum i Saruman)"/>
                <para PARENT="Lista wypunktowana cond (-1)" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="20 znaczników kontroli osad"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="1 znacznik postępów Drużyny Pierścienia"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="6 żetonów armii"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="1 znacznik zepsucia"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="2 żetony gracza prowadzącego (do rozgrywki 4-osobowej)"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="3 żetony Pierścieni Elfów"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="8 znaczników polityki Narodów"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="2 znaczniki punktów zwycięstwa"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="5 sześciościennych kości (używanych do liczenia punktów Poszukiwań)"/>
                <para PARENT="Lista wypunktowana cond" ParagraphEffectCharStyle="pogrubienie 11pt *"/>
                <ITEXT CH="Poniżej znajduje się lista komponentów, które należy wydrukować:"/>
                <para/>
